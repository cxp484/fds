&HEAD CHID='Waterloo_Methanol_GEOM_Predicted_0p5cm', TITLE='University of Waterloo Methanol Pool Burner, 0.5 cm resolution' /

&TIME T_END=60. /

&DUMP DT_SMOKE3D=0.01, DT_SLCF=1., DT_HRR=2. /

&MESH XB=-0.3,-0.15,-0.3,-0.15,-0.05,0.15, IJK=30,30,40, MULT_ID='mesh' /
&MULT ID='mesh', DX=0.15,DY=0.15,DZ=0.2, I_UPPER=3,J_UPPER=3,K_UPPER=3 / 64 mesh

&MISC SIMULATION_MODE='LES'/

&PRES VELOCITY_TOLERANCE=1.E-3, MAX_PRESSURE_ITERATIONS=100/

&REAC RADIATIVE_FRACTION=0.22, N_SIMPLE_CHEMISTRY_REACTIONS=2, FUEL_C_TO_CO_FRACTION=1.0, FUEL='METHANOL' /

&MATL ID='STEEL', CONDUCTIVITY=54, DENSITY=7800, SPECIFIC_HEAT=0.465, EMISSIVITY=0.8 /

&SURF ID='PAN', COLOR='GRAY 50', MATL_ID='STEEL', THICKNESS=0.003/

&SURF ID='POOL', COLOR='LIGHT BLUE', MATL_ID='METHANOL LIQUID', THICKNESS=0.15
      EXTERNAL_FLUX=20
      RAMP_EF='flux' /
&RAMP ID='flux', T= 0, F=1 /
&RAMP ID='flux', T=24, F=1 /
&RAMP ID='flux', T=25, F=0 /

&MATL ID                     = 'METHANOL LIQUID'
      EMISSIVITY             = 1.
      NU_SPEC                = 1.
      SPEC_ID                = 'METHANOL'
      HEAT_OF_REACTION       = 1099
      CONDUCTIVITY           = 0.20
      SPECIFIC_HEAT          = 2.48
      DENSITY                = 796
      ABSORPTION_COEFFICIENT = 1500
      BOILING_TEMPERATURE    = 64.65 /

&GEOM ID='Burner' SURF_ID='PAN','POOL' BINARY_FILE='../../../../cad/Case_Studies/Waterloo_Methanol/waterloo30cm.bingeom' /

&VENT MB='ZMIN', SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /

&PROP ID='TC', DIAMETER=0.0001 /

&CATF OTHER_FILES='Waterloo_Methanol_DEVC.fds'/

&SLCF PBY=0.001, QUANTITY='VELOCITY', VECTOR=T /
&SLCF PBY=0.001, QUANTITY='TEMPERATURE', CELL_CENTERED=T /
&SLCF PBY=0.001, QUANTITY='MIXING TIME', CELL_CENTERED=T /
&SLCF PBY=0.001, QUANTITY='UNMIXED FRACTION', CELL_CENTERED=T /
&SLCF PBY=0.001, QUANTITY='VOLUME FRACTION', SPEC_ID='OXYGEN', CELL_CENTERED=T /
&SLCF PBY=0.001, QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON DIOXIDE', CELL_CENTERED=T /
&SLCF PBY=0.001, QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON MONOXIDE', CELL_CENTERED=T /
&SLCF PBY=0.001, QUANTITY='VOLUME FRACTION', SPEC_ID='METHANOL', CELL_CENTERED=T /
&SLCF PBY=0.001, QUANTITY='EXTINCTION', CELL_CENTERED=T /

&BNDF QUANTITY='BURNING RATE', CELL_CENTERED=T /
&BNDF QUANTITY='GAUGE HEAT FLUX', CELL_CENTERED=T /
&BNDF QUANTITY='WALL TEMPERATURE', CELL_CENTERED=T/
&BNDF QUANTITY='MASS FLUX', SPEC_ID='METHANOL', CELL_CENTERED=T/

&TAIL /
