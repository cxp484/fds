&HEAD CHID='pine_10p5O2_40_1C', TITLE='Kashiwagi et al. Combust. Flame (1987), white pine gasification, 10.5 vol% O2, 40 kW/m2'/

&MESH IJK=20,20,20, XB=-0.05,0.05,-0.05,0.05,-0.05,0.05/

&TIME T_END=900./

&DUMP DT_DEVC=10., DT_PROF=10./

&VENT DB='ZMIN', SURF_ID='inflow'/
&VENT DB='ZMAX', SURF_ID='OPEN'/

&SURF ID='inflow', VEL=-0.005, COLOR='BLUE'/
&SURF ID='sample',         HT3D=T, MATL_ID='PINE', COLOR='BROWN'/
&SURF ID='exposed sample', HT3D=T, MATL_ID='PINE', COLOR='GRAY 10', EXTERNAL_FLUX=40./
&SURF ID='default wall', EMISSIVITY=0., HEAT_TRANSFER_COEFFICIENT=0., COLOR='INVISIBLE', FREE_SLIP=T, DEFAULT=T/

&OBST XB=0,0.038,-0.019,0.019,-0.019,0.019, SURF_ID6='exposed sample','sample','sample','sample','sample','sample'/

&SPEC ID='NITROGEN', BACKGROUND=T/
&SPEC ID='OXYGEN', MASS_FRACTION_0=0.115/ 10.5 vol % O2 to mass fraction
&SPEC ID='CARBON DIOXIDE'/
&SPEC ID='FUEL VAPOR'/

! simple material for setup
! &MATL ID='PINE', DENSITY=360., SPECIFIC_HEAT=1.15, CONDUCTIVITY=0.3 /

! full material model with kinetics
&CATF OTHER_FILES='../../../Utilities/Input_Libraries/MATL/pine_wood_1C_MATL.fds'/

&BNDF QUANTITY='MASS FLUX', CELL_CENTERED=T/
&BNDF QUANTITY='WALL TEMPERATURE', CELL_CENTERED=T/
&BNDF QUANTITY='BACK WALL TEMPERATURE', CELL_CENTERED=T/
&BNDF QUANTITY='TOTAL HEAT FLUX', CELL_CENTERED=T/
&BNDF QUANTITY='RADIATIVE HEAT FLUX', CELL_CENTERED=T/
&BNDF QUANTITY='CONVECTIVE HEAT FLUX', CELL_CENTERED=T/
&BNDF QUANTITY='WALL THICKNESS', CELL_CENTERED=T/
&BNDF QUANTITY='SURFACE DENSITY', CELL_CENTERED=T/

&DEVC XB=-0.05,0.05,-0.05,0.05,-0.05,0.05, QUANTITY='MASS FLUX', SURF_ID='sample', SPATIAL_STATISTIC='SURFACE INTEGRAL', ID='mf-sides'/
&DEVC XB=-0.05,0.05,-0.05,0.05,-0.05,0.05, QUANTITY='MASS FLUX', SURF_ID='exposed sample', SPATIAL_STATISTIC='SURFACE INTEGRAL', ID='mf-exposed'/

&DEVC XYZ=-0.001,0,0, QUANTITY='WALL TEMPERATURE', IOR=-1, ID='T0mm'/
&DEVC XYZ=-0.001,0,0, QUANTITY='INSIDE WALL TEMPERATURE', DEPTH=0.005, IOR=-1, ID='T5mm'/
&DEVC XYZ=-0.001,0,0, QUANTITY='INSIDE WALL TEMPERATURE', DEPTH=0.010, IOR=-1, ID='T10mm'/
&DEVC XYZ=-0.001,0,0, QUANTITY='INSIDE WALL TEMPERATURE', DEPTH=0.015, IOR=-1, ID='T15mm'/
&DEVC XYZ=-0.001,0,0, QUANTITY='BACK WALL TEMPERATURE', IOR=-1, ID='T38mm'/

&PROF ID='Tx', XYZ=-0.001,0,0, QUANTITY='INSIDE WALL TEMPERATURE', IOR=-1 /

&SLCF PBY=0, QUANTITY='VELOCITY', VECTOR=T/
&SLCF PBY=0, QUANTITY='TEMPERATURE', CELL_CENTERED=T/

&TAIL /
